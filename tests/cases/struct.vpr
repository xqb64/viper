struct person {
    name;
}

impl person {
    fn say(self, prefix, suffix) {
        self.do_say(prefix, suffix);
        return 0;
    }

    fn do_say(self, prefix, suffix) {
        print prefix ++ self.name ++ suffix;
        return 0;
    }
}

fn main() {
    let p = struct person { name: "John", };
    p.say("Hello, ", ".");
    return 0;
}

main();